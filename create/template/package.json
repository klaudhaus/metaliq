    "codegen": "graphql-codegen",
    "codegen-watch": "graphql-codegen --watch",
    "start": "npm run codegen-watch & npm run start-api & npm run start-app",
    "start-api": "metaliq run -f api/api-model apiModel",
    "start-app": "metaliq run"
