{
  "name": "metaliq",
  "version": "0.5.0",
  "type": "module",
  "main": "lib/meta.js",
  "types": "lib/meta.d.ts",
  "bin": {
    "metaliq": "bin/cli.cjs"
  },
  "scripts": {
    "dev": "tsc --watch",
    "install": "npm run local-or-peer-tsc",
    "local-or-peer-tsc": "( call npm run local-or-peer-tsc:win ) || npm run local-or-peer-tsc:nix",
    "local-or-peer-tsc:win": "if exist node_modules\\typescript tsc else (node ..\\..\\typescript\\bin\\tsc)",
    "local-or-peer-tsc:nix": "test ./node_modules/typescript && tsc || node ../../typescript/bin/tsc"
  },
  "author": "tstewart@klaudhaus.com",
  "license": "MIT",
  "description": "MetaliQ - A framework for specification-based meta-programming",
  "devDependencies": {
    "@metaliq/up": "^0.2.0",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "4",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "lit": "^2.0.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@metaliq/up": "^0.2.0",
    "lit": "^2.0.0"
  },
  "dependencies": {
    "@lit-labs/ssr": "^1.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-virtual": "^2.0.3",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mime-types": "^2.1.1",
    "@web/dev-server": "^0.1.25",
    "@web/dev-server-core": "^0.3.16",
    "clean-css": "^4.2.3",
    "commander": "^8.2.0",
    "esm-wallaby": "^3.2.25",
    "flatted": "^3.2.2",
    "fs-extra": "^10.0.0",
    "mime-types": "^2.1.33",
    "minify-html-literals": "^1.3.5",
    "path-to-regexp": "^6.2.0",
    "rollup": "^2.58.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "terser": "^5.9.0",
    "ts-dedent": "^2.2.0"
  }
}
